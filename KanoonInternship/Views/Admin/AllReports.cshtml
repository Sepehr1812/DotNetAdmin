
@{
    ViewData["Title"] = "All Reports";
}


<br />
<div class="container row p-0 m-0">
    <div class="col-6">
        <h2 class="text-info">All Reports</h2>
    </div>
    <div class="col-3 offset-3">
        <a asp-controller="Home" asp-action="Index" class="btn btn-info form-control text-white">Return to Panel</a>
    </div>
    <div class="col-12 border p-3">
        <table id="DT_load_r" class="table table-striped table-bordered" style="width:100%">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Writer</th>
                    <th>Date</th>
                    <th></th>
                </tr>
            </thead>
        </table>
    </div>
</div>

@section Scripts{
    <script src="~/js/ReportList.js"></script>
}
